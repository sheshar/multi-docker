{
  "AWSEBDockerrunVersion": 2, 
  "containerDefinitions": [
    {
      "name": "client",
      "image": "sheshar/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 512,
      "command": [
         "tail",
         "-f",
         "/bin/bash"
         ]
    },
    {
      "name": "server",
      "image": "sheshar/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 512,
      "command": [
         "tail",
         "-f",
         "/bin/bash"
         ]
    },
    {
      "name": "worker",
      "image": "sheshar/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 512,
      "command": [
         "tail",
         "-f",
         "/bin/bash"
         ]
    },
    {
      "name": "nginx",
      "image": "sheshar/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"],
      "memory": 512,
      "command": [
         "tail",
         "-f",
         "/bin/bash"
         ]
    }
  ]
} 
